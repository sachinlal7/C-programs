#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
        char arr[10];
    int i,p=0;

      scanf("%[^\n]s",arr);
    for(i=0;arr[i]!='\0';i++)
    {
        if(arr[i]=='r' && arr[i+1]=='a')
        p=1;
}
if(p==1)
printf("Yes");
else
printf("No");

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
